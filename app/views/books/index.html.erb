<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    
  </tbody>
  
</table>
<h2>New Book</h2>
  <%= form_with model: Book.new do |f| %>
    <h4>Title</h4>
    <%= f.text_field :title %>
    
    <h4>Body</h4>
    <%= f.text_area :body %>
    
    <%= f.submit 'Create Book' %>
  <% end %>
